{
  "name": "kurl",
  "version": "1.0.0",
  "description": "Kubernetes install script",
  "main": "./build/index.js",
  "license": "Apache License 2.0",
  "dependencies": {
    "aws-sdk": "^2.506.0",
    "body-parser": "^1.18.2",
    "bugsnag": "^2.0.1",
    "cors": "^2.8.1",
    "express": "^4.14.0",
    "express-rate-limit": "^2.11.0",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "^8.5.1",
    "monkit": "^0.4.0",
    "newrelic": "^5.7.0",
    "pino": "^4.10.4",
    "promise-mysql": "^4.1.0",
    "replicated-lint": "^0.13.3",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "sigsci-module-nodejs": "https://dl.signalsciences.net/sigsci-module-nodejs/1.4.8/sigsci-module-nodejs-1.4.8.tgz",
    "statsd-client": "^0.4.2",
    "superagent": "^5.1.0",
    "ts-express-decorators": "^5.24.1"
  },
  "devDependencies": {
    "@pact-foundation/pact": "^8.2.0",
    "@types/express": "^4.0.35",
    "@types/lodash": "^4.14.54",
    "@types/pino": "^4.7.1",
    "chai": "^4.1.2",
    "codeclimate-test-reporter": "^0.5.0",
    "coveralls": "^3.0.0",
    "env-subst": "^1.0.3",
    "mocha": "3.1.2",
    "mocha-jenkins-reporter": "^0.3.10",
    "mocha-junit-reporter": "^1.13.0",
    "mocha-typescript": "^1.0.23",
    "nyc": "^11.4.1",
    "snyk": "^1.41.1",
    "start-server-and-test": "^1.9.0",
    "ts-node": "^4.0.2",
    "tslint": "^4.5.1",
    "typemoq": "^2.1.0",
    "typescript": "^2.6.1"
  },
  "scripts": {
    "snyk-protect": "snyk protect",
    "start": "make build && bin/kurl serve",
    "test": "mocha --recursive --require ts-node/register src/test/**/*.ts",
    "api-tests": "mocha --recursive --require ts-node/register src/client/**/*.ts",
    "pact:provider:broker": "node ./pact/provider_tests_broker.js",
    "test:provider:broker": "start-server-and-test start http://localhost:3000 pact:provider:broker",
    "pact:provider:local": "node ./pact/provider_tests_local.js",
    "test:provider:local": "start-server-and-test start http://localhost:3000 pact:provider:local"
  },
  "snyk": true,
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.js"
    ],
    "reporter": [
      "text",
      "lcov"
    ],
    "all": true
  },
  "private": true
}
